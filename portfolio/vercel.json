{
  "version": 2,
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "src/**/*",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "api/*": {
      "src": "src/api/*.js"
    },
    "otherFunction": {
      "src": "src/otherFunction.js"
    }
    // Add more functions as needed
  }
}

